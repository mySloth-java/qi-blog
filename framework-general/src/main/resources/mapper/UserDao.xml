<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cg.mapper.UserDao">

    <!--security登陆验证 User Login(String username);-->
    <select id="Login" resultType="com.cg.entity.User">
        select * from users_tab where account = #{username}
    </select>

    <!--注册用户 Integer Register(UserDTO userDTO);-->
    <insert id="Register">
        insert into users_tab (id,account,password,name,create_time)
            values (#{id},#{account},#{password},#{name},#{createTime})
    </insert>

</mapper>
